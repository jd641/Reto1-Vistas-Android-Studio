<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet>
        <Constraint
            android:layout_height="80dp"
            motion:layout_constraintStart_toEndOf="@+id/imageView_Bg"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="277dp"
            android:id="@+id/button4" />
        <Constraint
            android:layout_height="match_parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="136dp"
            android:id="@+id/imageView_Bg" />
        <Constraint
            motion:layout_constraintVertical_bias="0.207"
            android:layout_height="28dp"
            motion:layout_constraintStart_toStartOf="@id/imageView_Bg"
            motion:layout_constraintTop_toTopOf="@id/imageView_Bg"
            motion:layout_constraintBottom_toBottomOf="@id/imageView_Bg"
            motion:layout_constraintHorizontal_bias="0.304"
            motion:layout_constraintEnd_toEndOf="@id/imageView_Bg"
            android:layout_width="300dp"
            android:id="@+id/monthTextView" />
        <Constraint
            motion:layout_constraintVertical_bias="0.442"
            android:layout_height="22dp"
            motion:layout_constraintStart_toStartOf="@id/imageView_Bg2"
            motion:layout_constraintTop_toTopOf="@id/imageView_Bg"
            motion:layout_constraintBottom_toTopOf="@id/imageView_Bg2"
            motion:layout_constraintHorizontal_bias="0.634"
            motion:layout_constraintEnd_toEndOf="@id/imageView_Bg2"
            android:layout_width="110dp"
            android:id="@+id/textView3" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="80dp"
            motion:layout_constraintStart_toEndOf="@+id/imageView_Bg"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="277dp"
            android:id="@+id/button4" />
    </ConstraintSet>
</MotionScene>